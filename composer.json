{
    "name": "muath-ye/dashboard",
    "description": "Do simple laravel helpers and dashboard with infoym adminLTE.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muathye",
            "email": "muath.ye@gmail.com"
        }
    ],
    "keywords": [
        "muath-ye",
        "dashboard",
        "laravel-helper"
    ],
    "minimum-stability": "dev",
    "require": {
        "nesbot/carbon": "^2.17",
        "infyomlabs/laravel-generator": "^3.0",
        "laravelcollective/html": "^6.2",
        "infyomlabs/adminlte-templates": "^3.0",
        "doctrine/dbal": "~2.3",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-buttons": "^4.10",
        "yajra/laravel-datatables-oracle": "~9.0",
        "spatie/laravel-permission": "^5.1",
        "appointer/swaggervel": "dev-master",
        "infyomlabs/swagger-generator": "dev-master"
    },
    "extra": {
    "laravel": {
      "providers": [
        "InfyOm\\Generator\\InfyOmGeneratorServiceProvider"
      ],
      "aliases": {
        "Form" : "Collective\\Html\\FormFacade",
        "Html" : "Collective\\Html\\HtmlFacade",
        "Flash" : "Laracasts\\Flash\\Flash"
      }
    }
  },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:InfyOmLabs/swaggervel.git"
        }
    ]
}
